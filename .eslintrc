{
  "parser": "babel-eslint",

  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },

  "globals": {
    "React": true
  },

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    },
    "sourceType": "module"
  },

  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "react/jsx-no-undef": 2,
    "new-cap": 0,
    "yoda": 0,
    "no-empty": 0,
    "no-extra-boolean-cast": 0,
    "consistent-return": 0
  },

  "plugins": [ "react", "jsx-a11y" ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.eslint.js"
      }
    }
  }
}
