{
  "presets": ["es2015", "stage-2", "stage-0", "react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }]
	],
  "env": {
    "production": {
      "plugins": ["transform-react-constant-elements"]
    },
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack/desktop/test.js", "verbose": true }]
      ]
    }
  }
}
